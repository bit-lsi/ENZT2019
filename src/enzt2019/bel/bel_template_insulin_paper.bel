##############
# Statements #
##############
SET Citation = {"PubMed", ""}

SET Evidence = "Dementia, a condition that frequently afflicts patients in advanced stages of Parkinson’s
disease (PD), results in decreased quality of life and survival time"
path(MESH:"Dementia") -- path(MESH:"Parkinson Disease")

SET Evidence = "The present study implicates the accumulation of
Lewy bodies in the cortex and limbic system as a potent trigger in the development
of PDD."
SET MeSHAnatomy = {"Limbic System", "Cerebral Cortex"}
SET MeSHDisease = "Parkinson Disease"
a(MESH:"Lewy Bodies") -> path(MESH:"Dementia")
UNSET MeSHAnatomy
UNSET MeSHDisease

SET Evidence = "In addition, significant Alzheimer-type pathologies, including amyloid-b (Ab)
plaques and NFTs, are observed in almost half of PDD patients."
SET MeSHDisease = "Parkinson Disease"
a(CHEBI:"amyloid-beta") pos path(MESH:"Dementia")
a(MeSH:"Neurofibrillary Tangles") pos path(MESH:"Dementia")
UNSET MeSHDisease

SET Evidence = "Interestingly, links
between PDD pathogenesis and the mechanisms underlying the development of insulin
resistance have begun to emerge."
SET MeSHDisease = "Parkinson Disease"
path(MESH:"Dementia") -- path(MESH:"Insulin Resistance")
UNSET MeSHDisease

SET Evidence = "Furthermore, previous studies have demonstrated that
insulin treatment reduces amyloid plaques in Alzheimer’s disease (AD), and normalizes
the production and functionality of dopamine and ameliorates motor impairments
in 6-OHDA-induced rat PD models."
SET Species = "10116"
SET MeSHDisease = "Parkinson Disease"
a(CHEBI:"insulin (human)") -| a(CHEBI:"amyloid-beta")
a(CHEBI:"insulin (human)") reg a(CHEBI:"dopamine")
a(CHEBI:"insulin (human)") -> a(GO:"motor behavior")
UNSET Species
UNSET MeSHDisease

SET Evidence = "GSK3b, a downstream substrate of PI3K/Akt
signaling following induction by insulin and IGF-1, exerts an influence on AD and PD
physiopathology"
bp(GO:"phosphatidylinositol 3-kinase signaling") -> p(HGNC:"GSK3B")
p(HGNC:"INS") -> bp(GO:"phosphatidylinositol 3-kinase signaling")
p(MESH:"Insulin-Like Growth Factor I") -> bp(GO:"phosphatidylinositol 3-kinase signaling")
p(HGNC:"GSK3B") -- path(MESH:"Parkinson Disease")
p(HGNC:"GSK3B") -- path(MESH:"Alzheimer Disease")

 SET Evidence = "The genetic overexpression of GSK3b in cortex and hippocampus
results in signs of neurodegeneration and spatial learning deficits in in vivo models
(Lucas et al., 2001), whereas its inhibition results in improvements in cognitive impairment
in these rodents, including AD and PD."
SET MeSHAnatomy = "Cerebral Cortex", "Hippocampus"
SET Species = "10116"
p(HGNC:"GSK3B") -> path(HP:"Neurodegeneration")
p(HGNC:"GSK3B") -| path(HP:"Spatial Learning")
p(HGNC:"GSK3B") -> path(MESH:"Cognitive Dysfunction")
